    <eAnnotations
        source="http://www.eclipse.org/emf/2002/Ecore/OCL">
      <details
          key="characteristicWildcardHasToBeOnLhsAndRhs"
          value="not self.rhs.oclIsKindOf(DataDictionaryCharacterized::expressions::CharacteristicReference) or&#xA;(&#xA;&#x9;self.rhs.oclAsType(DataDictionaryCharacterized::expressions::CharacteristicReference).characteristicType.oclIsUndefined()&#xA;&#x9;and&#xA;&#x9;self.lhs.characteristicType.oclIsUndefined()&#xA;)&#xA;or&#xA;(&#xA;&#x9;not self.rhs.oclAsType(DataDictionaryCharacterized::expressions::CharacteristicReference).characteristicType.oclIsUndefined()&#xA;&#x9;and&#xA;&#x9;not self.lhs.characteristicType.oclIsUndefined()&#xA;)"/>
      <details
          key="literalWildcardHasToBeOnLhsAndRhs"
          value="not self.rhs.oclIsKindOf(DataDictionaryCharacterized::expressions::CharacteristicReference) or&#xA;(&#xA;&#x9;self.rhs.oclAsType(DataDictionaryCharacterized::expressions::CharacteristicReference).literal.oclIsUndefined()&#xA;&#x9;and&#xA;&#x9;self.lhs.literal.oclIsUndefined()&#xA;)&#xA;or&#xA;(&#xA;&#x9;not self.rhs.oclAsType(DataDictionaryCharacterized::expressions::CharacteristicReference).literal.oclIsUndefined()&#xA;&#x9;and&#xA;&#x9;not self.lhs.literal.oclIsUndefined()&#xA;)"/>
      <details
          key="onlyUseCompatibleCharacteristicReferences"
          value="self.oclAsType(ecore::EObject)->closure(e : ecore::EObject |&#xA;&#x9;e.eContents()->selectByKind(ecore::EObject)&#xA;)->including(self.rhs.oclAsType(ecore::EObject))->selectByKind(DataDictionaryCharacterized::expressions::CharacteristicReference)->forAll(r |&#xA;&#x9;r.oclIsKindOf(DataDictionaryCharacterized::expressions::ContainerCharacteristicReference) or&#xA;&#x9;r.oclIsKindOf(expressions::ExternalCallActionCharacteristicReference) or&#xA;&#x9;r.oclIsKindOf(expressions::SeffParameterCharacteristicReference) or&#xA;&#x9;r.oclIsKindOf(expressions::SeffReturnCharacteristicReference)&#xA;)"/>
    </eAnnotations>
